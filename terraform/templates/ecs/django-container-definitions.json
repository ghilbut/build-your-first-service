[
  {
    "name": "${name}",
    "image": "${image}",
    "essential": true,
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 8000
      }
    ],
    "entryPoint": ["./manage.py", "runserver", "0:8000"],
    "environment": ${environment},
    "secrets": ${secrets}
  }
]

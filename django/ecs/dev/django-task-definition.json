{
  "family": "byfs-dev-django",
  "executionRoleArn": "arn:aws:iam::869061964712:role/iam-role-byfs-ecs-execution",
  "networkMode": "awsvpc",
  "containerDefinitions": [
  {
    "name": "django",
    "image": "ghilbut/byfs:latest",
    "essential": true,
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 8000
      }
    ],
    "entryPoint": [
      "./manage.py",
      "runserver",
      "0:8000"
    ],
    "environment": [
      {
        "name": "DJANGO_SETTINGS_MODULE",
        "value": "byfs.settings.dev"}
    ],
    "secrets": [
      {
        "name": "BYFS_DEV_DB_HOST",
        "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-dev-mysql-host-0Nvw4m"
      },
      {
        "name": "BYFS_DEV_DB_PORT",
        "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-dev-mysql-port-nNnXam"
      },
      {
        "name": "BYFS_DEV_DB_NAME",
        "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-dev-mysql-name-FJXA0i"
      },
      {
        "name": "BYFS_DEV_DB_USER",
        "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-dev-mysql-username-RJUOxj"
      },
      {
        "name": "BYFS_DEV_DB_PASSWORD",
        "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-dev-mysql-password-KLgrhk"
      }
    ]
  }
]
,
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "tags": [
    {
      "key": "Name",
      "value": "ecs-byfs-dev-django-task-definition"
    },
    {
      "key": "organization",
      "value": "platform"
    },
    {
      "key": "owner",
      "value": "terraform"
    },
    {
      "key": "stage",
      "value": "dev"
    },
    {
      "key": "terraform:environment",
      "value": "develop"
    },
    {
      "key": "terraform:workspace",
      "value": "dev"
    }
  ]
}
